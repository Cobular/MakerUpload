    "svelte-fragment-component": "^1.2.0",
    "svelte-htm": "^1.2.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.29.2"
