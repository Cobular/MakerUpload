    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitest/coverage-c8": "^0.29.7",
    "autoprefixer": "^10.4.7",
    "daisyui": "^2.18.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
